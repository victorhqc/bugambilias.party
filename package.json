{
  "name": "salon-bugambilias-static",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "NODE_ENV=production next build",
    "start": "next start",
    "eslint": "eslint src",
    "test": "jest",
    "deploy": "rm -rf .next/ && NODE_ENV=production serverless deploy --aws-profile=bugambilias-party",
    "deploy-production": "stage=prod npm run deploy -- --stage prod",
    "deploy-staging": "stage=staging npm run deploy -- --stage staging"
  },
  "serverlessNextJS": {
    "hooks": {
      "after:build": "node ./cleanBuild.js"
    }
  },
  "dependencies": {
    "@githubprimer/octicons-react": "^8.5.0",
    "@reach/router": "^1.2.1",
    "@smooth-ui/core-sc": "^9.0.1",
    "chalk": "^2.4.2",
    "color": "^3.1.0",
    "dotenv": "^7.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^6.0.0",
    "next": "^8.0.3",
    "next-compose-plugins": "^2.1.1",
    "next-optimized-images": "^2.4.0",
    "next-seo": "^1.11.2",
    "prop-types": "^15.7.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-hot-loader": "^4.6.5",
    "react-spring": "^8.0.7",
    "serverless-api-compression": "^1.0.1",
    "styled-components": "^4.1.3",
    "ua-parser-js": "^0.7.19",
    "webp-loader": "^0.4.0"
  },
  "devDependencies": {
    "compression-webpack-plugin": "^2.0.0",
    "eslint": "^5.3.0",
    "eslint-config-healthier": "^1.0.2",
    "eslint-config-react-tools": "1.x.x",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "serve": "10.1.1",
    "serverless-nextjs-plugin": "^1.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    },
    "sideEffects": false
  }
}
